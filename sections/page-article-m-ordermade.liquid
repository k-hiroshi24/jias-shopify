}

/* キャッシュへパラメーターを保存 */
function setCacheParam (_param, _line_type) {
    var _d = 24 * 60 * 60;
    document.cookie = _line_type + "=" + _param + ";max-age=" + _d;
}

/* キャッシュ確認*/
function chkCacheParam (_line_type) {
    var _cache = document.cookie;
    var _cache_array = _cache.split(';');
    var _target_cache = false;

    $.each(_cache_array, function(_index, _value){
        var _cache_param = _value.split('=');
        if(_cache_param[0] == _line_type || _cache_param[0] == " " + _line_type){
            _target_cache = _cache_param[1];
        }
    });

    return _target_cache;
}
  

/* パラメーター確認 */
function setLINEURI() {
    // URLを取得
    let _url = new URL(window.location.href);

    // URLSearchParamsオブジェクトを取得
    let _params = _url.searchParams;

    // 各種LINE用パラメータ取得
    var _LINE_simulation_param = _params.get('linetypeS');
    var _LINE_coupon_param = _params.get('linetypeC');
    var _LINE_showroom_param = _params.get('linetypeL');

    /** シミュレーション訴求指定あり **/
    if(_LINE_simulation_param != null && _LINE_simulation_param != "none"){
        /* 変更するリンクをリストアップ */
        var _link_list = $('a.line_simulation');
      
        /* キャッシュへパラメーターをセット */
        setCacheParam(_LINE_simulation_param, "line_simulation");

        /* リンクセット */
        setLineLink(_LINE_simulation_param, _link_list);
    } else {
        _LINE_simulation_param = chkCacheParam("line_simulation");

        if(_LINE_simulation_param) { 
            /* 変更するリンクをリストアップ */
            var _link_list = $('a.line_simulation');
          
            /* 広告遷移のキャッシュが残っていればリンクセット */
            setLineLink(_LINE_simulation_param, _link_list);
        }
    }

  
    /** クーポン訴求指定あり **/
    if(_LINE_coupon_param != null && _LINE_coupon_param != "none"){
        /* 変更するリンクをリストアップ */
        var _link_list = $('a.line_coupon');
      
        /* キャッシュへパラメーターをセット */
        setCacheParam(_LINE_coupon_param, "line_coupon");

        /* リンクセット */
        setLineLink(_LINE_coupon_param, _link_list);
    } else {
        _LINE_simulation_param = chkCacheParam("line_coupon");

        if(_LINE_simulation_param) { 
            /* 変更するリンクをリストアップ */
            var _link_list = $('a.line_coupon');
          
            /* 広告遷移のキャッシュが残っていればリンクセット */
            setLineLink(_LINE_simulation_param, _link_list);
        }
    }

    /** LINEショールーム予約指定あり **/
    if(_LINE_showroom_param != null && _LINE_showroom_param != "none"){
        /* 変更するリンクをリストアップ */
        var _link_list = $('a.line_showroom');
      
        /* キャッシュへパラメーターをセット */
        setCacheParam(_LINE_showroom_param, "line_showroom");

        /* リンクセット */
        setLineLink(_LINE_showroom_param, _link_list);
    } else {
        _LINE_simulation_param = chkCacheParam("line_showroom");

        if(_LINE_simulation_param) { 
            /* 変更するリンクをリストアップ */
            var _link_list = $('a.line_showroom');
          
            /* 広告遷移のキャッシュが残っていればリンクセット */
            setLineLink(_LINE_simulation_param, _link_list);
        }
    }

}

