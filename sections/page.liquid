{% render 'breadcrumbs' %}
<article class="site-page" data-template-page>
  <header
    class="
      page-masthead
      {% if page.content == blank %}mgb-0{% endif %}
      ">
    <h1
      class="
        page-title
        {% if page.content == blank %}mgb-0{% endif %}
      ">
      {{ page.title }}
    </h1>
  </header>

  {% if page.content != blank %}
    <div class="page-content rte">
      {{ page.content }}
    </div>
  {% endif %}
</article>
